<!DOCTYPE html>
<html lang="en">
  <%- include("partials/head.ejs") %> 
<body>
  <header>
    <%- include("partials/header.ejs") %> 
  </header>
  <main>
    <div class="container">
    <div class="row">
      <% for (let i = 0; i < 3; i++) { %>
        <div class="col">
          <a href="/pokemon/<%=i%>">
            <img src="<%=pokemon[i].img%>" alt="<%=pokemon[i].name%> ">
          </a>
        </div>
      <% } %>  
    </div>
    </div>
 
  </main>
</body>
</html>